  const uint8_t h_roineData[] = {9, 20, 1, 0, 0, 0xFF, 1, 0x99, 0x93,0x33, 0x99,0x99, 0x99,0x33, 0x39,0x99, 0x99,0x33, 0x30,0x99, 0x99,0x93, 0x33,0x33, 0x99,0x93, 0x3b,0xb3, 0x39,0x99, 0x35,0x35, 0x53,0x99, 0x35,0xd5, 0x55,0xd5, 0x33,0x55, 0x3b,0x35, 0x53,0xd5, 0x53,0xb3, 0x55,0xd5, 0x95,0x3b, 0x35,0x95, 0x59,0x53, 0xb3,0x59, 0x55,0x9d, 0xdd,0xdd, 0x95,0x59, 0x53,0xb3, 0x59,0x5d, 0x95,0x3b, 0x35,0x9d, 0x39,0x53, 0xb3,0x59, 0x39,0x93, 0x99,0x93, 0x99,0x99, 0x59,0x99, 0x59,0x99, 0x95,0x99, 0x95,0x99, 0x99,0xd9, 0x99,0xd9, 0x99,0x9d, 0x99,0x9d, 0x99};
Image h_roine_Image(h_roineData);

void setup() {
  gb.begin();
}

void loop() {
  while(!gb.update());
  
//ENTREE
  if(gb.buttons.repeat(BUTTON_RIGHT, 0)) {  //déplacer personnage a droite
     playerPosition_x = playerPosition_x + 2;  
  }

  if(gb.buttons.repeat(BUTTON_LEFT, 0)) {
    playerPosition_x = playerPosition_x - 2;
  }


  
//MISE A JOUR

//AFFICHAGE     //note a moi-meme : animer la carte
  gb.display.drawImage(0, 0, map_testData);  //carte
  gb.display.drawImage(playerPosition_x, 26, h_roineData);  //héroine
  
  gb.display.drawImage(3, 0, coeurData);  //coeur de vie
  gb.display.drawImage(9, 0, coeurData);  //coeur de vie
  gb.display.drawImage(15, 0, coeurData);  //coeur de vie
  
  gb.display.drawImage(52, 0, nourritureData);  //nourriture
  gb.display.drawImage(58, 0, nourritureData);  //nourriture
  gb.display.drawImage(64, 0, nourritureData);  //nourriture





}
